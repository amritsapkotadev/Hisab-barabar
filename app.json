{
  "expo": {
    "name": "Hisab Kitab",
    "slug": "hisab-kitab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.amrit.expensetracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
   
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.amrit.expensetracker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1204d3bc-61a4-474f-bdaa-9c30a683ed39"
      }
    },
    "owner": "amritsapkota"
  }
}
